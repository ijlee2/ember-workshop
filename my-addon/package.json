{
  "name": "my-addon",
  "version": "1.0.0",
  "private": true,
  "description": "An Ember addon to illustrate core concepts in Ember (v6)",
  "keywords": [
    "emberjs",
    "ember-addon",
    "embroider"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/ember-workshop.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    },
    "./addon-main.js": "./addon-main.cjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "declarations/*"
      ]
    }
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "addon-main.cjs",
    "declarations",
    "dist",
    "translations"
  ],
  "scripts": {
    "addon": "blueprints-v2-addon --test-app-location \"../my-app\"",
    "build": "rollup --config",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input --cache",
    "lint:css:fix": "stylelint \"**/*.css\" --allow-empty-input --fix",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:intl": "ember-intl-lint",
    "lint:intl:fix": "ember-intl-lint --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prelint:types": "type-css-modules --src src",
    "lint:types": "ember-tsc --noEmit",
    "prepack": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo \"A v2 addon does not have tests, run tests in my-app\""
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.10.2",
    "decorator-transforms": "^2.3.1",
    "ember-container-query": "^6.1.5",
    "ember-modifier": "^4.2.2",
    "ember-truth-helpers": "^5.0.0",
    "embroider-css-modules": "^3.1.5",
    "tracked-built-ins": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/plugin-transform-typescript": "^7.28.6",
    "@babel/runtime": "^7.28.6",
    "@ember-intl/lint": "^0.4.1",
    "@embroider/addon-dev": "^8.2.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/ember-tsc": "^1.0.9",
    "@glint/template": "^1.7.3",
    "@glint/tsserver-plugin": "^2.0.9",
    "@ijlee2-frontend-configs/ember-template-lint": "^2.3.2",
    "@ijlee2-frontend-configs/eslint-config-ember": "^2.5.0",
    "@ijlee2-frontend-configs/prettier": "^2.3.3",
    "@ijlee2-frontend-configs/stylelint": "^2.2.2",
    "@rollup/plugin-babel": "^6.1.0",
    "@tsconfig/ember": "^3.0.12",
    "babel-plugin-ember-template-compilation": "^4.0.0",
    "blueprints-v2-addon": "workspace:*",
    "concurrently": "^9.2.1",
    "ember-intl": "^8.0.3",
    "ember-source": "~6.9.0",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.39.2",
    "postcss": "^8.5.6",
    "prettier": "^3.8.0",
    "rollup": "^4.55.1",
    "rollup-plugin-postcss": "^4.0.2",
    "stylelint": "^16.26.1",
    "type-css-modules": "^2.1.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "ember-intl": "^8.0.3"
  },
  "engines": {
    "node": "20.* || >= 22"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "app-js": {},
    "main": "addon-main.cjs",
    "public-assets": {},
    "type": "addon",
    "version": 2
  }
}
