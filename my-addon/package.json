{
  "name": "my-addon",
  "version": "1.0.0",
  "private": true,
  "description": "An Ember addon to illustrate core concepts in Ember (v6)",
  "keywords": [
    "emberjs",
    "ember-addon",
    "embroider"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/ember-workshop.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    },
    "./addon-main.js": "./addon-main.cjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "declarations/*"
      ]
    }
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "addon-main.cjs",
    "declarations",
    "dist",
    "translations"
  ],
  "scripts": {
    "addon": "blueprints-v2-addon --test-app-location \"../my-app\"",
    "build": "rollup --config",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\" --cache",
    "lint:css:fix": "stylelint \"**/*.css\" --fix",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:intl": "ember-intl-lint",
    "lint:intl:fix": "ember-intl-lint --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prelint:types": "type-css-modules --src src",
    "lint:types": "ember-tsc --noEmit",
    "prepack": "rollup --config",
    "start": "concurrently \"pnpm:start:*\" --names \"start:\"",
    "start:js": "rollup --config --watch --no-watch.clearScreen",
    "start:types": "glint --declaration --watch",
    "test": "echo \"A v2 addon does not have tests, run tests in my-app\""
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.10.2",
    "decorator-transforms": "^2.3.0",
    "ember-container-query": "^6.1.4",
    "ember-modifier": "^4.2.2",
    "ember-truth-helpers": "^5.0.0",
    "embroider-css-modules": "^3.1.3",
    "tracked-built-ins": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@ember-intl/lint": "^0.2.0",
    "@embroider/addon-dev": "^8.1.2",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/ember-tsc": "^1.0.8",
    "@glint/template": "^1.7.3",
    "@glint/tsserver-plugin": "^2.0.8",
    "@ijlee2-frontend-configs/ember-template-lint": "^2.3.0",
    "@ijlee2-frontend-configs/eslint-config-ember": "^2.4.1",
    "@ijlee2-frontend-configs/prettier": "^2.3.1",
    "@ijlee2-frontend-configs/stylelint": "^2.2.0",
    "@rollup/plugin-babel": "^6.1.0",
    "@tsconfig/ember": "^3.0.12",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "blueprints-v2-addon": "workspace:*",
    "concurrently": "^9.2.1",
    "ember-intl": "^8.0.0",
    "ember-source": "~6.8.2",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.39.1",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "rollup": "^4.53.3",
    "rollup-plugin-postcss": "^4.0.2",
    "stylelint": "^16.26.1",
    "type-css-modules": "^2.1.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "ember-intl": "^8.0.0"
  },
  "engines": {
    "node": "20.* || >= 22"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "app-js": {
      "./components/navigation-menu.js": "./dist/_app_/components/navigation-menu.js",
      "./components/ui/page.js": "./dist/_app_/components/ui/page.js"
    },
    "main": "addon-main.cjs",
    "public-assets": {},
    "type": "addon",
    "version": 2
  }
}
