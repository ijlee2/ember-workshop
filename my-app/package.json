{
  "name": "my-app",
  "version": "1.0.0",
  "private": true,
  "description": "An Ember app to illustrate core concepts in Ember (v6)",
  "keywords": [
    "ember",
    "emberjs",
    "ember-polaris",
    "ember-tutorial",
    "embroider"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/ember-workshop.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\" --cache",
    "lint:css:fix": "stylelint \"**/*.css\" --fix",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:intl": "ember-intl-lint",
    "lint:intl:fix": "ember-intl-lint --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prelint:types": "type-css-modules --src app",
    "lint:types": "ember-tsc --noEmit",
    "start": "vite",
    "test": "vite build --mode development && ember test --path dist --test-port=0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.6",
    "@ember-intl/lint": "^0.4.1",
    "@ember-intl/vite": "^0.4.0",
    "@ember/optional-features": "^2.3.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.4.1",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/compat": "^4.1.12",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.4.2",
    "@embroider/macros": "^1.19.6",
    "@embroider/router": "^3.0.6",
    "@embroider/vite": "^1.5.0",
    "@faker-js/faker": "^10.2.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/ember-tsc": "^1.0.9",
    "@glint/template": "^1.7.3",
    "@glint/tsserver-plugin": "^2.0.9",
    "@ijlee2-frontend-configs/ember-template-lint": "^2.3.2",
    "@ijlee2-frontend-configs/eslint-config-ember": "^2.5.0",
    "@ijlee2-frontend-configs/prettier": "^2.3.3",
    "@ijlee2-frontend-configs/stylelint": "^2.2.2",
    "@nullvoxpopuli/ember-composable-helpers": "^5.3.0",
    "@rollup/plugin-babel": "^6.1.0",
    "@tsconfig/ember": "^3.0.12",
    "@types/qunit": "^2.19.13",
    "@types/sinon": "^21.0.0",
    "autoprefixer": "^10.4.23",
    "babel-plugin-ember-template-compilation": "^4.0.0",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.1",
    "ember-a11y-refocus": "^5.1.0",
    "ember-a11y-testing": "^7.1.3",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~6.9.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^7.0.0",
    "ember-cli-netlify": "^0.4.1",
    "ember-concurrency": "^5.1.0",
    "ember-container-query": "^6.1.5",
    "ember-intl": "^8.0.3",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.9.0",
    "ember-template-lint": "^7.9.3",
    "ember-test-selectors": "^7.1.0",
    "ember-truth-helpers": "^5.0.0",
    "embroider-css-modules": "^3.1.5",
    "eslint": "^9.39.2",
    "miragejs": "^0.1.48",
    "my-addon": "workspace:*",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.8.0",
    "qunit": "^2.25.0",
    "qunit-dom": "^3.5.0",
    "sinon": "^21.0.1",
    "stylelint": "^16.26.1",
    "tracked-built-ins": "^4.1.0",
    "type-css-modules": "^2.1.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "engines": {
    "node": "20.* || >= 22"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  }
}
