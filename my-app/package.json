{
  "name": "my-app",
  "version": "1.0.0",
  "private": true,
  "description": "An Ember app to illustrate core concepts in Ember (v6)",
  "keywords": [
    "ember",
    "emberjs",
    "ember-polaris",
    "ember-tutorial",
    "embroider"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/ember-workshop.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\" --cache",
    "lint:css:fix": "stylelint \"**/*.css\" --fix",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prelint:types": "type-css-modules --src app",
    "lint:types": "glint",
    "start": "ember serve",
    "test": "ember test --test-port=0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.3.0",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/broccoli-side-watch": "^1.1.0",
    "@embroider/compat": "^3.9.1",
    "@embroider/core": "^3.5.7",
    "@embroider/legacy-inspector-support": "^0.1.3",
    "@embroider/macros": "^1.19.1",
    "@embroider/router": "^2.1.9",
    "@embroider/webpack": "^4.1.1",
    "@faker-js/faker": "^10.0.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.5.2",
    "@glint/environment-ember-loose": "^1.5.2",
    "@glint/environment-ember-template-imports": "^1.5.2",
    "@glint/template": "^1.5.2",
    "@ijlee2-frontend-configs/ember-template-lint": "^2.2.0",
    "@ijlee2-frontend-configs/eslint-config-ember": "^2.3.0",
    "@ijlee2-frontend-configs/prettier": "^2.2.0",
    "@ijlee2-frontend-configs/stylelint": "^2.1.1",
    "@nullvoxpopuli/ember-composable-helpers": "^5.3.0",
    "@tsconfig/ember": "^3.0.11",
    "@types/qunit": "^2.19.13",
    "@types/sinon": "^17.0.4",
    "autoprefixer": "^10.4.21",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.2.1",
    "ember-a11y-refocus": "^5.1.0",
    "ember-a11y-testing": "^7.1.2",
    "ember-auto-import": "^2.11.1",
    "ember-cli": "~6.7.1",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mirage": "^3.0.4",
    "ember-cli-netlify": "^0.4.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-concurrency": "^5.1.0",
    "ember-container-query": "^6.0.4",
    "ember-intl": "^8.0.0-beta.2",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.7.0",
    "ember-template-imports": "^4.3.0",
    "ember-template-lint": "^7.9.3",
    "ember-test-selectors": "^7.1.0",
    "ember-truth-helpers": "^5.0.0",
    "embroider-css-modules": "^3.0.2",
    "eslint": "^9.37.0",
    "loader.js": "^4.7.0",
    "miragejs": "^0.1.48",
    "my-addon": "workspace:*",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.6.2",
    "qunit": "^2.24.2",
    "qunit-dom": "^3.5.0",
    "sinon": "^21.0.0",
    "stylelint": "^16.25.0",
    "tracked-built-ins": "^4.0.0",
    "type-css-modules": "^2.0.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1"
  },
  "engines": {
    "node": "20.* || >= 22"
  },
  "ember": {
    "edition": "octane"
  }
}
