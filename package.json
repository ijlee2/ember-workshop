{
  "name": "ember-workshop",
  "version": "1.0.0",
  "private": true,
  "description": "A demo app to illustrate core and latest concepts in Ember.js",
  "keywords": [
    "ember",
    "emberjs",
    "ember-octane",
    "ember-polaris",
    "ember-tutorial"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/ember-workshop.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"app/**/*.css\" --cache",
    "lint:css:fix": "stylelint \"app/**/*.css\" --fix",
    "lint:dependency": "ember dependency-lint",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember test"
  },
  "resolutions": {
    "@embroider/macros": "^1.13.0",
    "@embroider/util": "^1.12.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.9",
    "@babel/plugin-proposal-decorators": "^7.22.7",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.1.0",
    "@ember/string": "3.1.1",
    "@ember/test-helpers": "^3.2.0",
    "@faker-js/faker": "^8.0.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.0",
    "ember-a11y-refocus": "^3.0.2",
    "ember-a11y-testing": "^6.1.0",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "~4.12.1",
    "ember-cli-app-version": "^6.0.1",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-dependency-lint": "^2.0.1",
    "ember-cli-deprecation-workflow": "^2.1.0",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mirage": "^2.4.0",
    "ember-cli-netlify": "^0.4.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^3.1.0",
    "ember-container-query": "^4.0.5",
    "ember-css-modules": "^2.0.1",
    "ember-data": "~4.12.3",
    "ember-fetch": "^8.1.2",
    "ember-intl": "^5.7.2",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^7.0.0",
    "ember-resolver": "^11.0.0",
    "ember-sinon-qunit": "^7.1.4",
    "ember-source": "~4.12.3",
    "ember-template-lint": "^5.11.1",
    "ember-template-lint-plugin-prettier": "^5.0.0",
    "ember-test-selectors": "^6.0.0",
    "ember-truth-helpers": "^3.1.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-ember": "^11.10.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-qunit": "^8.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "loader.js": "^4.7.0",
